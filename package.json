{
  "name": "blockly-tool",
  "version": "1.0.0",
  "description": "A Blockly tooling project",
  "type": "module",
  "imports": {
    "#app/*.js": "./app/*.js",
    "#src/*.js": "./src/*.js",
    "#test/*.js": "./test/*.js"
  },
  "main": "index.js",
  "scripts": {
    "start": "vite --force",
    "test": "node --test",
    "test-watch": "node --test --watch",
    "lint": "eslint src/",
    "lint-export": "eslint export/",
    "build": "npm run export && vite build",
    "build-all-branches": "node build_all_branches.js",
    "preview": "npm run build && vite preview",
    "export": "node export.js"
  },
  "keywords": [
    "blockly"
  ],
  "author": "Loren Norman",
  "license": "",
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "chai": "^5.1.2",
    "eslint": "^8.57.0",
    "esm-reload": "^1.0.1",
    "glob": "^10.4.2",
    "globals": "^15.2.0",
    "vite": "^4.4.9"
  },
  "dependencies": {
    "@blockly/theme-modern": "^5.0.5",
    "blockly": "^10.1.3",
    "lodash-es": "^4.17.21"
  }
}
